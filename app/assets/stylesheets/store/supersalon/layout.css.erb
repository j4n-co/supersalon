.top_search_form{
	display: inline-block;
	position: relative;
}
#keywords {
	padding-top: 0.5em;
	text-align: left;
	border: none;
	font-size: 15px;
	font-family: 'Calibre-Medium';
	width: 49pxw;
	letter-spacing: 1px;
	border: none;
	outline: none;

}
#keywords:focus{
		font-family: 'Calibre-Medium';
}
#keywords ~ input{
  width: 20px;
  height: 16px;
  padding: 0;
  border: none;
  background-color: #FFF;
  color: rgba(0, 0, 0, 0);
  position: absolute;
  font-family: 'Calibre-Medium';
  top: 0.5em;
  right:  0;
  cursor: pointer;
  -webkit-transition: none;
  -moz-transition: none;
  -o-transition: none;
  -ms-transition: none;
  transition: none;
  display: none;
  /*
  background: url('/assets/lupa.png') no-repeat center center transparent; 
*/
}

#keywords::-webkit-input-placeholder{
	color: black;
}
#keywords::-moz-input-placeholder{
	color: black;	
}
.top_search_form #keywords{
	 border-bottom: 2px solid transparent; 
	   width: 5.5em;
}
.top_search_form #keywords:hover{
}

.top_search_form #keywords:focus{
  -webkit-transition: none;
  -moz-transition: none;
  -o-transition: none;
  -ms-transition: none;
  transition: none;	
  border-color:black; 
}
.top_search_form #keywords:not(:focus){
  -webkit-transition: none;
  -moz-transition: none;
  -o-transition: none;
  -ms-transition: none;
  transition: none;		
}

