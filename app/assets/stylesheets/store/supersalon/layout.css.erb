	float: left;
	bottom: 0;
}
#next > .nav-inner{
	float: right;
	text-align: right;
}

#next > .nav-inner > .nav-inner-text{
	right: 0;
}

#prev > .nav-inner{
	float: left;
	text-align: left;
}

#next:hover .nav-inner, #prev:hover .nav-inner{
	visibility: visible;
}

#next{
